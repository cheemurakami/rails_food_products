<!DOCTYPE html>
<html>
  <head>
    <title>RailsFoodProducts</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <div class="container">
        <nav class="navbar navbar-dark bg-dark">
          <a class="navbar-brand" href="/">Home</a>
          <a class="navbar-brand" href="/products/new">Add Product</a>
          <a class="navbar-brand" href="/users/sign_in">Sign in</a>
          <a class="navbar-brand" href="/users/sign_up">Sign up</a>
          <a class="navbar-brand" href="/users/sign_out" data-method="delete" value="<%= destroy_user_session_path%>">Sign Out</a>
        </nav>
            <%= yield %>
        </div>
      </div>
  </body>
</html>
